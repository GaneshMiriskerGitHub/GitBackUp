package packagee19_Dynamic_Programming;

public class A2_Print_All_Combinations_of_Balanced_Parenthesis {
	
	public static void printParantheses(int n, int cClose, int cOpen, String str) {
		if(str.length() == 2*n) {
			System.out.println(str);
			return;
		}
		
		if(cOpen > cClose) {
			//choice1 : include open
			printParantheses(n-1, cClose, cOpen+1, str+"(");
			//choice2 : include close
			printParantheses(n-1, cClose+1, cOpen, str+")");
		}else {
			
		}
	}

	public static void main(String[] args) {
		
		int 

	}

}
