public class A4_Longest_Increasing_Path_In_Matrix {

	

	public static void main(String[] args) {

		int mat[][] = { { 1, 2, 3, 4 }, { 2, 2, 3, 4 }, { 3, 2, 3, 4 }, { 4, 5, 6, 7 }, };

		int n = 4, m = 4;
		System.out.println(wrapper(mat, n, m));

	}

}
